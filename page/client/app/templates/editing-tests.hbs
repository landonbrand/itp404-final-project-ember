{{outlet}}
<div id="sidebar" {{action 'deselect'}}>
  <div id="treeViewer" {{action 'onChange'}}>
    <div class='tree-view-box' contenteditable='true'><span>
      {{selectedTags.data.[2].nodeName}}
      <div class='tree-view-box' contenteditable='true'><span>
        {{selectedTags.data.[1].nodeName}}
        <div class='tree-view-box' contenteditable='true'><span>
          {{selectedTags.data.[0].nodeName}}
          <div class='tree-view-box' contenteditable='true'><span>

          </span></div>
        </span></div>
      </span></div>
    </span></div>
  </div>
  <button {{action 'h1'}}>H1</button>
  <button {{action 'noStyle'}}>No Style</button>
</div>

<div class="container" {{action 'mouseUpOnEdits'}}>
  <div class="page">
    <div id="edit" contenteditable="true">
      {{{model}}}
    </div>
  </div>
</div>

<button {{action 'save'}}>Save</button>
<button {{action 'cancelEdits'}}>Cancel Editing</button>
